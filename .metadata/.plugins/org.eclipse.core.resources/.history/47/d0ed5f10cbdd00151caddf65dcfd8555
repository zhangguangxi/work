package com.job;

/**
 * TODO:类功能介绍
 * @version 2016年2月28日上午11:06:58
 * @author guangxi.zhang
 */
public class Biz {
    
    static int count =100;
    
    public static void jian(){
        synchronized (Biz.class) {
            count--;
            System.out.println(count);
            if(count == 0){
                // 销毁线程池
                System.exit(0);
            }
        }
    }
}
